                <div class="card" style="margin-left:122px;">
                    <div class="card-head card-topline-aqua">
                        <center> <header>Doctor Details</header></center>
                    </div>
                    <div class="card-body no-padding height-5">

                        <ul class="list-group list-group-unbordered">
                            <li class="list-group-item">
                                <b>Dr Name </b>
                                <div class="profile-desc-item pull-right">@Model.Name</div>
                            </li>
                            <li class="list-group-item">
                                <b>Gender </b>
                                <div class="profile-desc-item pull-right">@Model.Gender</div>
                            </li>
