                                <b>Mobile Number </b>
                                <div class="profile-desc-item pull-right">@Model.Mobile</div>
                            </li>
                            <li class="list-group-item">
                                <b>Qualification</b>
                                <div class="profile-desc-item pull-right">@Model.Qualification</div>
                            </li>
                            <li class="list-group-item">
                                <b>Special List</b>
                                <div class="profile-desc-item pull-right">@Model.Specialist</div>
                            </li>
                            <li class="list-group-item">
                                <b>Timings</b>
